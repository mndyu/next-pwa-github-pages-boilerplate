{
  "name": "next-pwa-github-pages",
  "version": "0.1.0",
  "license": "MIT",
  "baseUrl": "/next-pwa-github-pages",
  "scripts": {
    "dev": "next dev",
    "dev-nosw": "NO_SW=true next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "next build && next export && touch out/.nojekyll && gh-pages --dist out --dotfiles true",
    "type-check": "standard && tsc",
    "format": "standard --fix",
    "test": "standard && jest"
  },
  "dependencies": {
    "next": "latest",
    "next-pwa": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "gh-pages": "^3.1.0",
    "standard": "^16.0.3",
    "typescript": "^4.0.5"
  },
  "standard": {
    "ignore": [
      "/public"
    ]
  }
}
